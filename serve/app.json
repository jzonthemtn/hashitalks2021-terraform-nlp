[{
  "essential": true,
  "image": "jzemerick/ner-serving:latest",
  "memory": 4096,
  "memoryReservation": 4096,
  "name": "model-name",
  "portMappings": [{
    "containerPort": 8080,
    "hostPort": 0
  }],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "nlp-serving",
      "awslogs-region": "us-east-1",
      "awslogs-stream-prefix": "my-model/final-model.pt"
    }
  },
  "environment": [{
      "name": "MODEL_BUCKET",
      "value": "${bucket}"
    },
    {
      "name": "MODEL_KEY",
      "value": "${key}"
    }
  ]
}]
